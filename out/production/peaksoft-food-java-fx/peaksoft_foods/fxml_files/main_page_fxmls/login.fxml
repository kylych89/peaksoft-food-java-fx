<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="191.0" prefWidth="371.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="peaksoft_foods.controllers.LoginController">
   <children>
      <TextField fx:id="txtLogin" layoutX="59.0" layoutY="41.0" prefHeight="25.0" prefWidth="255.0" promptText="Логин жазыңыз" />
      <TextField fx:id="txtPassword" layoutX="58.0" layoutY="81.0" prefHeight="25.0" prefWidth="255.0" promptText="Сыр сөзүңүздү жазыңыз" />
      <ButtonBar layoutX="111.0" layoutY="128.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="btnEnter" defaultButton="true" mnemonicParsing="false" onAction="#onButtonClicked" text="Кирүү" />
            <Button fx:id="btnCancel" cancelButton="true" layoutX="135.0" layoutY="18.0" mnemonicParsing="false" onAction="#onButtonClicked" text="Баш тартуу" />
        </buttons>
      </ButtonBar>
      <Label layoutX="75.0" layoutY="14.0" prefHeight="17.0" prefWidth="221.0" text="Логин жана сыр сөзүңүздү киргизиңиз" />
   </children>
</AnchorPane>
